#include<iostream>
using namespace std;

static void subset(int* list, int i = 0, int n = 2)
{
	if (i > n)
	{
		copy(list, list + n + 1, ostream_iterator<int>(cout, ""));
		cout << endl;
	}
	else
	{
		list[i++] = 0;
		subset(list, i--, n);
		list[i++] = 1;
		subset(list, i, n);
	}
}

int main()
{
	int a[3] ;
	subset(a,0,2);
}
